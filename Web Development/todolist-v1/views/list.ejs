<%- include("header") -%>

<div class="box" id="heading">
  <h1><%= kindOfDay %></h1>
</div>


<div class="box">
  <% items.forEach(function(item) { %>
  <div class="item">
    <input type="checkbox">
    <p> <%= item %> </p>
  </div>
  <% }); %>

  <form class="item" action="/" method="post">
    <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
    <button type="submit" name="button">+</button>
  </form>
</div>

<%- include("footer") -%>
